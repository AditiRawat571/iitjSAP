<div class="align-card d-flex align-items-center justify-content-center upload">
  <div class="card" style="width:400px">
  
    
  
  

    <div class="card-body" id="upload">
      <h1>UPLOAD</h1>
      <form action="/notes" class="was-validated" method="post">
        <div class="row">
          <div class="col"><div class="mb-3 mt-3">
            <label for="fname" class="form-label">Firstname:</label>
            <input type="text" class="form-control" id="fname" placeholder="Enter First Name" name="fname" required>
            <div class="valid-feedback">Valid.</div>
            <div class="invalid-feedback">Please fill out this field.</div>
          </div></div>
          <div class="col"><div class="mb-3 mt-3">
            <label for="lname" class="form-label">Lastname:</label>
            <input type="text" class="form-control" id="lname" placeholder="Enter Last Name" name="lname" required>
            <div class="valid-feedback">Valid.</div>
            <div class="invalid-feedback">Please fill out this field.</div>
          </div></div>
          
        </div>
        
        <div class="row">
          <div class="col"><div class="mb-3 mt-3">
            <label for="roll" class="form-label">Roll No:</label>
            <input type="text" class="form-control" id="roll" placeholder="Enter Roll No" name="roll" required>
            <div class="valid-feedback">Valid.</div>
            <div class="invalid-feedback">Please fill out this field.</div>
          </div></div>
          <div class="col"><div class="mb-3 mt-3">
            <label for="email" class="form-label">Email:</label>
            <input type="email" class="form-control" id="email" placeholder="Email" name="email" required>
            <div class="valid-feedback">Valid.</div>
            <div class="invalid-feedback">Please fill out this field.</div>
          </div></div>
          
        </div>
        
        <div class="mb-3 mt-3">
          <label for="dlink" class="form-label">Link:</label>
          <input type="text" class="form-control" id="dlink" placeholder="Enter Drive Link" name="dlink" required>
          <div class="valid-feedback">Valid.</div>
          <div class="invalid-feedback">Please fill out this field.</div>
        </div>
         
       
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>



</div>
  
